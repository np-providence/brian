{
  "version": 2,
  "host":["http://localhost:8080"],
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": [
        "*"
      ],
      "expose_headers": [
        "Content-Length"
      ],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "HEAD",
        "POST"
      ]
    }
  },
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "Face It API Gateway",
  "endpoints": [
    {
      "endpoint": "/try",
      "method": "GET",
      "extra_config": {},
      "output_encoding": "json",
      "concurrent_calls": 1,
      "backend": [
        {
          "url_pattern": "/testing",
          "encoding": "json",
          "sd": "static",
          "extra_config": {},
          "method": "GET",
          "host": [
            "http://identifier:5000"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}
